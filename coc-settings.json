{
  // "coc.preferences.jumpCommand": "split",
  "diagnostic.infoSign": "âš ",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "typescript",
    "typescriptreact"
  ],
  "eslint.autoFixOnSave": true,
  "html.suggest.angular1": false,
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck"
  },
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "javascript.jsx": ["javascript"]
  },
  "coc.source.word.priority": 100,
  "tabnine.priority": 99,
  "list.source.mru.ignoreGitIgnore": true,
  "emmet.includeLanguages": {
    "vue-html": "html",
    "ejs": "html",
    "javascript": "javascriptreact"
  },
  "snippets.textmateSnippetsRoots": ["~/.config/nvim/vscode-snippets"],
  "json.format.enable": false,
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  // "suggest.selection": "first",
  "tabnine.enablePreselect": false,
  "suggest.enablePreselect": false,
  "suggest.noselect": true
}
