{
  // "coc.preferences.jumpCommand": "split",
  "diagnostic.infoSign": "I",
  "diagnostic.hintSign": "H",
  "diagnostic.errorSign": "E",
  "diagnostic.warningSign": "W",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "typescript",
    "typescriptreact"
  ],
  "eslint.autoFixOnSave": true,
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck"
  },
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "javascript.jsx": ["javascript"],
    "typescriptreact": ["javascript"],
    "typescript": ["javascript"],
    "tpyescript.jsx": ["javascript"],
    "vue": ["javascript"]
  },
  "coc.source.word.priority": 70,
  "coc.source.buffer.priority": 80,
  "list.source.mru.ignoreGitIgnore": true,
  "emmet.includeLanguages": {
    "vue-html": "html",
    "ejs": "html",
    // "javascript": "javascriptreact"
  },
  "json.format.enable": false,
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "suggest.selection": "first",
  "suggest.enablePreselect": false,
  "suggest.noselect": true,
  "python.formatting.provider": "autopep8",
  "snippets.ultisnips.pythonPrompt": false,
  "pyright.inlayHints.functionReturnTypes": false,
  "pyright.inlayHints.variableTypes": false,
  "volar.takeOverMode.enabled": false,
  "volar.enable": true,
  "volar.autoClosingTags": true,
  "vetur.enable": false
}
